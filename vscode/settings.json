{
    "workbench.colorTheme": "City Lights",
    "editor.fontFamily": "InconsolataLGC Nerd Font Mono",
    "editor.fontSize": 18,
    "editor.lineHeight":30,
    "editor.renderWhitespace": "none",
    "files.autoSave": "onFocusChange",
    "emmet.includeLanguages": {
        "javascript": "javascriptreact"
    },
    "window.zoomLevel": 0,
    "editor.formatOnPaste": true,
    "terminal.integrated.fontFamily": "InconsolataLGC NF",
    "terminal.integrated.fontSize": 16,
    "editor.cursorBlinking": "smooth",
    "vim.startInInsertMode": false,
    "vim.statusBarColorControl": true,
    "vim.easymotion": true,
    "vim.sneak": true,
    "vim.incsearch": true,
    "vim.useSystemClipboard": true,
    "vim.useCtrlKeys": true,
    "vim.hlsearch": true,
    "vim.easymotionMarkerBackgroundColor": "#03827b"  ,
    "vim.easymotionMarkerForegroundColorOneChar": "#ffffff",
    "vim.easymotionMarkerForegroundColorTwoChar":"#ffffff",
    "vim.easymotionMarkerWidthPerChar": "12",
    "vim.easymotionMarkerHeight":"20",
    "vim.easymotionMarkerFontFamily":"InconsolataLGC NF",
    "vim.easymotionMarkerFontSize":"18",
    "vim.easymotionMarkerFontWeight":"bold",
    "vim.insertModeKeyBindings": [
        {
            "before": ["j", "j"],
            "after": ["<Esc>"]
        },
        {
            "before":["k","k"],
            "after": ["<Esc>"]
       }
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
        "before": [";"],
        "after": [":"]
        },
        {
        "before": ["<leader>","n"],
        "commands": [":noh"]
        },
        {
            "before": ["s"],
            "after": ["leader", "leader", "s"]
        },
        {
            "before":["="],
            "after": ["leader","leader","leader","j"]
        },
    ],
    "vim.leader": "<space>",
    "vim.handleKeys": {
        "<C-a>": false,
        "<C-z>": false,
    },
    "vim.statusBarColors.insert": "#4c535e",
    "vim.statusBarColors.normal": "#158e72",
    "vim.statusBarColors.visual": "#5f2975",
    "vim.statusBarColors.replace": "#ff9696",
    "vim.statusBarColors.visualblock": "#87064d",
    "vim.statusBarColors.visualline": "#869124",
    "workbench.colorCustomizations": {
        "statusBar.background": "#158e72",
        "statusBar.noFolderBackground": "#158e72",
        "statusBar.debuggingBackground": "#158e72"
    },
    "terminal.integrated.rendererType": "dom",
    "workbench.startupEditor": "newUntitledFile",
    "terminal.integrated.shell.windows": "C:\\Windows\\System32\\wsl.exe",
    "window.title": "${activeEditorLong}${separator}${rootName}",
    "workbench.iconTheme": "material-icon-theme",
    "window.menuBarVisibility": "toggle",
    "workbench.editor.enablePreview": false

}
